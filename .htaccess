RewriteEngine On
RewriteCond %{HTTPS} !=on
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301,NE]
Header always set Content-Security-Policy "upgrade-insecure-requests"


# Define custom error pages
ErrorDocument 404 /404NotFound.php
ErrorDocument 500 /404NotFound.php

# Rewrite rules for clean URLs
RewriteRule ^Home$ index.php [L]
RewriteRule ^Accommodation$ accommodation.php [L]   
RewriteRule ^Aplinetentdetails$ aplinetentdetails.php [L]
RewriteRule ^Beytdwarkamandir$ Beytdwarkamandir.php [L]
RewriteRule ^BeytDwarkaParikrama$ BeytDwarkaParikrama.php [L]
RewriteRule ^Contact$ contact.php [L]  
RewriteRule ^SiteMap$ sitemap.php [L]  



